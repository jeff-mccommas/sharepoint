{
    "loader": "eslint-loader",
    "options": {
        "failOnWarning": false
    },

    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {

        "quotes": [1, "double", "avoid-escape"]
    }
}